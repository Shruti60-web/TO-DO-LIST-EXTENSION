{

  "manifest_version": 2,
  
  "name": "To-Do List ",
  
  "description": "This extension will help to manage your daily tasks",
  
  "version": "1.0",
  
  "browser_action": {
  
  "default_popup": "popup.html"
  },

  
  "background":{
    "scripts": ["background.js"]
  },
  

    

    
  
  "icons": { "16": "logo.png" },
  
  "permissions": [
  
  "tabs",
  
  "alarms",
  
  "storage",

  "notifications",

  "background"
  
  ]
  
  }
  
  
